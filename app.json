{
  "expo": {
    "name": "tutor_app",
    "slug": "tutor_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#C26ED2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.medschoolspace.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.medschoolspace.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7f733eab-9243-470a-8a5c-b1a530614d84"
      }
    },
    "owner": "junkvjacob"
  }
}
